      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'строковые-фильтры-(line-filters)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'директории-(directories)';
          }
          
      }
  </script>
