      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'чтение-файлов-(reading-files)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'строковые-фильтры-(line-filters)';
          }
          
      }
  </script>
