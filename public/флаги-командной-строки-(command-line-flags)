  <script>
      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'аргументы-командной-строки-(command-line-arguments)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'подкоманды-командной-строки-(command-line-subcommands)';
          }
          
      }
  </script>
  <body>
