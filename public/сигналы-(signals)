  <script>
      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'исполняющие-процессы-(execing-processes)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'выход-(exit)';
          }
          
      }
  </script>
