  <script>
      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'флаги-командной-строки-(command-line-flags)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'переменные-среды-(environment-variables)';
          }
          
      }
  </script>
