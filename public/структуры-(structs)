      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'указатели-(pointers)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'методы-(methods)';
          }
          
      }
  </script>
