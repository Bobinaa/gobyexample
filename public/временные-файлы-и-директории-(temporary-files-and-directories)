      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'директории-(directories)';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'тестирование-(testing)';
          }
          
      }
  </script>
